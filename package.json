{
    "name": "swaglint",
    "version": "0.0.1",
    "description": "Yet another swagger linter",
    "main": "index.js",
    "directories": {
        "bin": "./bin/",
        "doc": "./doc/",
        "test": "./test/"
    },
    "scripts": {
        "build": "$(npm bin)/tsc && $(npm bin)/babel ./lib/ --out-dir ./lib/",
        "test": "npm run test:style",
        "test:style": "$(npm bin)/tslint ./src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bycedric/swaglint.git"
    },
    "keywords": [
        "open",
        "api",
        "swagger",
        "lint"
    ],
    "author": "Cedric van Putten <me@bycedric.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bycedric/swaglint/issues"
    },
    "homepage": "https://github.com/bycedric/swaglint#readme",
    "devDependencies": {
        "@types/node": "7.0",
        "babel-cli": "^6.24",
        "babel-preset-env": "^1.3",
        "tslint": "^5.1",
        "typescript": "^2.2"
    }
}
